! My Xresources dot file.
! Written by Tiger Sachse.

! *** COLORS ***
! BLACK
*.color0: #2e3436
*.color8: #555753

! RED
*.color1: #a40000
*.color9: #ef2929

! GREEN
*.color2: #4e9a06
*.color10: #7cd227

! YELLOW
*.color3: #c4a000
*.color11: #e9d63e

! BLUE 
*.color4: #3465a4
*.color12: #6596cb

! MAGENTA
*.color5: #75507b
*.color13: #b280ac

! CYAN
*.color6: #ce5c00
*.color14: #fcaf3e

! WHITE
*.color7: #babdb9
*.color15: #eeeeec
*.foreground: #ffffff
*.cursorColor: #ffffff

! *** GENERAL ***
*.scrollTtyOutput: False
*.scrollTtyKeypress: True

! *** VISUALS ***
*.depth: 32
*.scrollBar: False
*.cursorBlink: True
*.internalBorder: 20
*.cursorUnderline: True
*.font: xft:ubuntumono:size=14
*.background: rgba:0825/0825/0825/c800

! *** COPY/PASTE ***
*.iso14755: false
*.iso14755_52: false
*.keysym.Shift-Control-V: eval:paste_clipboard
*.keysym.Shift-Control-C: eval:selection_to_clipboard

! *** EXTENSIONS ***
*.perl-ext-common: default,matcher
*.url-launcher: /usr/bin/xdg-open
*.matcher.rend.0: Uline Bold fg14
*.matcher.button: 1
