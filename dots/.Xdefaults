! My Xresources dot file.
! Written by Tiger Sachse.

! *** COLORS ***
! BLACK
*.color0: #252525
*.color8: #454545
! RED
*.color1: #FF8D80
*.color9: #FF9F95
! GREEN
*.color2: #A6E22E
*.color10: #B6E354
! YELLOW
*.color3: #FD971F
*.color11: #FCA542
! BLUE
*.color4: #789EC6
*.color12: #46A4FF
! MAGENTA
*.color5: #D182CD
*.color13: #E67AE0
! CYAN
*.color6: #69CFC6
*.color14: #4DE3D6
! WHITE
*.color7: #DBDCDC
*.color15: #FDFDFD
! BACKGROUND/FOREGROUND
*.background: [90]#151515
*.foreground: #FFFFFF

! *** GENERAL ***
*.scrollTtyOutput: False
*.scrollTtyKeypress: True

! *** VISUALS ***
*.depth: 32
*.scrollBar: False
*.cursorBlink: True
*.internalBorder: 20
*.cursorUnderline: True
*.font: xft:sharetechmono:size=14

! *** COPY/PASTE ***
*.iso14755: false
*.iso14755_52: false
*.keysym.Shift-Control-V: eval:paste_clipboard
*.keysym.Shift-Control-C: eval:selection_to_clipboard

! *** EXTENSIONS ***
*.perl-ext-common: default,matcher
*.url-launcher: /usr/bin/xdg-open
*.matcher.rend.0: Uline Bold fg14
*.matcher.button: 1
